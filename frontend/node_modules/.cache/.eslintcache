nt/PropertyComponent/propertyCartApi'\r\n\r\nfunction BuyerComponent () {\r\n  const [showlocation, setShowlocation] = useState([]),\r\n    [token, setToken] = useState(false),\r\n    [form, setForm] = useState(false),\r\n    [showProperty, setShowProperty] = useState([]),\r\n    [showPropertyType,setShowPropertyType] = useState([]);\r\n  const API = 'http://localhost:3333/realestate'\r\n\r\n  const handleChange = e => {\r\n    console.log('Handle change')\r\n    console.log('NAME & VALUES: ', e.target.value)\r\n  }\r\n\r\n  const profile = () => {\r\n    return (\r\n      <>\r\n        <div>\r\n          <header>\r\n            <div className='row header bg-info bg-opacity-75'>\r\n              <div className='col-2 my-4 my-2 text-dark'>\r\n                <p className='mx-3 fs-4 my-2 fw-bold font-monospace'>\r\n                  Real Estate\r\n                </p>\r\n              </div>\r\n              <div className='col-2 location my-2'>\r\n                <select\r\n                  className='form-select rounded-pill'\r\n                  onChange={e => handleChange(e)}\r\n                >\r\n                  {showlocation.map(item => (\r\n                    <option\r\n                      value={item.location}\r\n                      key={item._id}\r\n                      onClick={e => handleChange(e)}\r\n                      name='locations'\r\n                    >\r\n                      {item.location}\r\n                    </option>\r\n                  ))}\r\n                </select>\r\n              </div>\r\n              <div className='col-2 location my-2'>\r\n                <select\r\n                  className='form-select rounded-pill'\r\n                  onChange={e => handleChange(e)}\r\n                >\r\n                  <option onChange={e => handleChange(e)}>Property type</option>\r\n                  {showProperty.map((item, index) => (\r\n                    <option>{item.propertyFor} </option>\r\n                  ))}\r\n                </select>\r\n              </div>\r\n              {token ? authenticated() : entryProfile()}\r\n            </div>\r\n          </header>\r\n        </div>\r\n      </>\r\n    )\r\n  }\r\n\r\n  // Guest Login\r\n  const entryProfile = () => {\r\n    return (\r\n      <React.Fragment>\r\n        <div className='col d-flex flex-row-reverse py-3 px-4'>\r\n          <Link\r\n            to={'/realestate/signup'}\r\n            className='d-flex flex-row-reverse text-decoration-none'\r\n          >\r\n            <button className='btn bg-white' onClick={e => setForm(!form)}>\r\n              Login / Signup\r\n            </button>\r\n          </Link>\r\n        </div>\r\n      </React.Fragment>\r\n    )\r\n  }\r\n\r\n  // Authenication\r\n  const authenticated = () => {\r\n    return (\r\n      <React.Fragment>\r\n        <div className='col start'>\r\n          <div className='col d-flex flex-row-reverse py-3 px-4'>\r\n            <FaUser className='cursor-pointer fs-1' id='usericon' />\r\n            <p className='my-2 mx-2 fs-5'>You </p>\r\n          </div>\r\n        </div>\r\n      </React.Fragment>\r\n    )\r\n  }\r\n\r\n  // Get location data\r\n  const cityMaster = () => {\r\n    const url = `${API}/showlocation`\r\n\r\n    fetch(url)\r\n      .then(async res => {\r\n        const fetchData = await res.json()\r\n        return fetchData\r\n      })\r\n      .then(fetchData => {\r\n        setShowlocation(fetchData.data.location)\r\n        console.log('fetchData.data.location: ', fetchData.data.location)\r\n      })\r\n      .catch(err => {\r\n        console.log('Show location: ', err.message)\r\n      })\r\n  }\r\n\r\n  // Buyer...!\r\n  const tokenValidate = () => {\r\n    if (JSON.parse(localStorage.getItem('token'))) {\r\n      setToken(true)\r\n    }\r\n  }\r\n\r\n  // Get property\r\n  const getProperty = () => {\r\n    console.log('City master...!')\r\n    fetch(`${API}/showpropertyfor`)\r\n      .then(async res => {\r\n        const fetchData = await res.json()\r\n        return fetchData\r\n      })\r\n      .then(fetchData => {\r\n        setShowProperty(fetchData.data.data)\r\n        console.log('Show property for : ', fetchData.data.data)\r\n      })\r\n      .catch(err => {\r\n        console.log('Show location: ', err.message)\r\n      })\r\n  }\r\n\r\n  const getPropertyType = async (path) => {\r\n    await fetch(`${API}/${path}`)\r\n      .then(async res => {\r\n        const fetchData = await res.json();\r\n        return fetchData;\r\n      })\r\n      .then(fetchData => {\r\n        console.log('Get property type: ',fetchData)\r\n        setShowPropertyType(fetchData.data.data);\r\n      })\r\n      .catch(err => {\r\n        console.log(\"Get property type: \", err);\r\n      })\r\n  }\r\n\r\n  useEffect(e => {\r\n    cityMaster()\r\n    tokenValidate()\r\n    getProperty()\r\n    getPropertyType('showpropertytype')\r\n  }, [])\r\n\r\n  return (\r\n    <>\r\n      {profile()}\r\n      <PropertyCart />\r\n    </>\r\n  )\r\n}\r\n\r\nexport default BuyerComponent\r\n",[],"D:\\Claritaz\\Real-Estate\\frontend\\src\\Components\\AdminComponent\\AdminDashboard\\admindashboard.js",[],[],"D:\\Claritaz\\Real-Estate\\frontend\\src\\Components\\AdminComponent\\AdminMgmtSellerComponent\\adminSellerMgmt.js",[],[],"D:\\Claritaz\\Real-Estate\\frontend\\src\\Components\\AdminComponent\\AdminMasterComponent\\adminmasterapi.js",[],[],"D:\\Claritaz\\Real-Estate\\frontend\\src\\Components\\NavigateComponent\\router.js",[],[],"D:\\Claritaz\\Real-Estate\\frontend\\src\\Components\\UserComponent\\PropertyComponent\\PropertyComponent\\propertyCartApi.js",[],[],{"ruleId":"81","severity":1,"message":"82","line":12,"column":6,"nodeType":"83","messageId":"84","endLine":12,"endColumn":22},"no-unused-vars","'showPropertyType' is assigned a value but never used.","Identifier","unusedVar"]